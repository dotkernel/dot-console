{
    "name": "dotkernel/dot-console",
    "type": "library",
    "description": "DotKernel component for creating console applications based on laminas-console",
    "license": "MIT",
    "homepage": "https://github.com/dotkernel/dot-console",
    "keywords": [
        "console",
        "dotkernel",
        "laminas",
        "mezzio"
    ],
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "dotkernel/dot-log": "^3.1",
        "dotkernel/dot-errorhandler": "^3.1",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-console": "^2.8",
        "laminas/laminas-dependency-plugin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Console\\": "tests"
        }
    }
}
